{
  "_version": "1.73.1",
  "sap.app": {
    "id": "soheader",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.5",
      "toolsId": "228275f9-0443-47bb-b1a5-4544d8e151b9"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZSB_SOH/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.136.8",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.mdc": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "soheader.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "soheader.view",
        "async": true
      },
      "routes": [
      { 
        "name": "RouteView1",
        "pattern": ":?query:",
        "target": ["TargetView1"]
      }
    ],
    "targets": {
      "TargetView1": {
            "viewName": "View1",
            "viewId": "View1",
            "viewType": "XML",
            "viewLevel": 1
          }
        }
      },
    "rootView": {
      "viewName": "soheader.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
